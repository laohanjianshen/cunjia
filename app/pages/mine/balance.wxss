/* pages/mine/balance.wxss */
page{
	background: #fff;
}
.money-progress{
	font-size:28rpx;
	color:#fc9c02;
	text-align: right;
	margin: 24rpx 24rpx 28rpx 0;
}
.wrap image{
	width: 152rpx;
	height: 152rpx;
	margin: 0px auto 14rpx;
	display: block;
}
.money-num{
	font-size:48rpx;
	color:#d15454;
	line-height: 90rpx;
	text-align: center;
	margin-bottom: 32rpx;
}
.money-num text{
	font-size: 64rpx;
}
.info{
	font-size:28rpx;
	color:#999999;
	text-align: center;
	margin-bottom: 92rpx;
}
.btn{
	background:#fc9c02;
	border-radius:12rpx;
	width:686rpx;
	height:94rpx;
	text-align: center;
	line-height: 94rpx;
	font-size:36rpx;
	color:#ffffff;
	margin: 0 auto 40rpx;
}
.btn2{
	border:2rpx solid #fc9c02;
	background: #fff;
	color:#fc9c02;
}